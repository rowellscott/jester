<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Jester</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootswatch/3.3.7/lumen/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/style.css">
    <!-- Emojis -->
    <link href="https://afeld.github.io/emoji-css/emoji.css" rel="stylesheet">
    
    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="crossorigin="anonymous"></script>
   
     <!-- Axios -->
    <!-- <script src="https://unpkg.com/axios/dist/axios.min.js"></script> -->
    
    <!-- Local API Handler -->
    <!-- <script type="text/javascript" src="../javascripts/APIHandler.js"></script>
    <script type="text/javascript" src="../javascripts/jokesAPI.js"></script> -->
    <script type="text/javascript" src="../javascripts/ratings.js"></script>
  </head>
  <body id="jokes">
      <nav class="navbar">
          <div class="container-fluid top">
            <div class="navbar-header">
              <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#home-nav" aria-expanded="false">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
            </div>
    
            <div class="collapse navbar-collapse" id="home-nav">
              <ul class="nav navbar-nav">
                <li class="jester-logo navbar-brand">Jester</li>
                <li class="nav-logo">
                  <!-- <a href="#" class="nav-logo-link">
                    <img class="nav-logo-image" src="https://s3-eu-west-1.amazonaws.com/ih-materials/uploads/upload_8c904a3bee158b96cdaf0cad6900b4b2.png" alt="Kickstarter Logo">
                  </a> -->
                </li>
              </ul>
              <ul class="nav navbar-nav navbar-right">
                <!-- <li><a href="/jokes">All</a></li> -->
                <li><h5><a href="/jokes/<%= user._id %>">MyJokes</a></h5></li>
                <li><h5><a href="/jokes/new">Add A Joke</a></h5></li>
                <li><h5><a href="/favorites/<%= user._id %>">Favorites</a></h5></li>
                <li>
                  <form action="/logout" method="post">
                    <button class="btn btn-link logout" type="submit">Logout</button>
                  </form>
                </li>
              </ul>
            </div>
          </div>
        </nav>
    <div class="jokes-container">
        <div class="container left-container">
            <!-- Link To List All Jokes -->
            <!-- <h5 class="username">Hi <%= user.username %>!</h5> -->
            <h5><a id="all-link" href="/jokes">All Jokes</a></h5>
            <!-- Search By Rating -->
            <!-- <h5><a href="#">Rating</a></h5> -->
            <!-- Search Bar -->
            <form id="search" action="/jokes/search" method="POST">
              <label for="search-input" class="search-label">Keyword Search:</label>
              <input id="search-input" type="text" name="query" class="form-control">
              <button class="btn search-button" type="submit" id="search-button">Search</button>
            </form>
            <br>
            
            <!-- Categories Dropdown Menu -->
            <div class="dropdown" aria-labelledby="dropdownMenuButton">  
              <a class="dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                <span class="categories-link">Categories</span>
                <span class="caret"></span>
              </a>
              <div class="dropdown-menu categories">
                <ul>
                <% categories.forEach((category)=>{ %>
                  <li>
                  <a class="dropdown-item" id="<%= category %>" href="/jokes/categories/<%= category %>"><%= category %></a>
                  </li>
               <% }) %>
                </ul>
              </div>
            </div>
            <br>
            <br>
            <br>
        </div>
      <%- body -%>
    </div>

    <!-- <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script> -->
