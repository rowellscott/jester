
        <div class="container">
          <div class="row">
            <div class="col-sm-6 text-center app-info">
             <h1 class="app-info-text">Welcome to Jester!</h1>
             <h1 class="app-info-text">Find Jokes and Share with Your Friends!</h1>
            </div>
            <div class="col-sm-6 jumbotron" id="sign-up-form">
              <h2>Sign Up</h2>
              <% if (typeof message !== "undefined"){ %> 
                <div class="error-message"><%= message %></div>
              <% } %>
              <form action="/signup" method="POST">
                <div class="form-group">
                    <label for="email">Email</label>
                    <input type="text" name="email" class="form-control">
                  </div>
                <div class="form-group">
                  <label for="username">Username</label>
                  <input type="text" name="username" class="form-control">
                </div>
                <div class="form-group">
                  <label for="password">Password</label>
                  <input type="password" name="password" class="form-control">
                </div>
                <div class="form-group">
                  <label for="firstName">Name</label>
                  <input type="text" name="firstName" class="form-control">
                </div>
                <div class="form-group">
                  <label for="lastName">Last Name</label>
                  <input type="text" name="lastName" class="form-control">
                </div>
                <div class="form-group">
                  <label for="dob">Date of Birth</label>
                  <input type="date" name="dob" class="form-control">
                </div>
                
                
                <button class="btn btn-default" type="submit">Sign Up</button>
              </form>
              <br>
              <div class="sign-up-form-bottom">
                <p style="color: white; text-align: center"> Already have an account?<a href="/login" class="text-primary">Login</a></p>
              </div>
            </div>
          </div>
        </div>

       
